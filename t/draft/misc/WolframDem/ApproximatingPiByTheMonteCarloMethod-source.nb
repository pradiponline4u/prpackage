      "Body" :> (
       SeedRandom[1234]; $CellContext`data = 
        RandomReal[{0, 1}, {$CellContext`a$$, 2}]; $CellContext`dataCircle = 
        Select[$CellContext`data, (0.5 - Part[#, 1])^2 + (0.5 - Part[#, 2])^2 <= 
