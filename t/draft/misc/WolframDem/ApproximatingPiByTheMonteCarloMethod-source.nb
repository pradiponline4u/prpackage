          RGBColor[0.5 + Mod[$CellContext`b$$, 2], 
            Mod[$CellContext`b$$, 3], 
            Mod[$CellContext`b$$, 5]]]; $CellContext`p2 = Graphics[{Blue, 
           PointSize[0.007 $CellContext`c$$], 
           Point[$CellContext`complement]}, Background -> 
          RGBColor[0.5 + Mod[$CellContext`b$$, 2], 
            Mod[$CellContext`b$$, 3], 
            Mod[$CellContext`b$$, 5]]]; $CellContext`app = 
        4 (Length[$CellContext`dataCircle]/$CellContext`a$$); Column[{
