Cell[BoxData[
 RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"SeedRandom", "[", "1234", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"data", "=", 
     RowBox[{"RandomReal", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "2"}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"dataCircle", "=", 
     RowBox[{"Select", "[", 
      RowBox[{"data", ",", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"0.5", "-", 
             RowBox[{"#", "[", 
              RowBox[{"[", "1", "]"}], "]"}]}], ")"}], "^", "2"}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"0.5", "-", 
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}]}], ")"}], "^", "2"}]}], 
         "\[LessEqual]", 
         RowBox[{
          RowBox[{"(", "0.5", ")"}], "^", "2"}]}], "&"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"complement", "=", 
     RowBox[{"Complement", "[", 
      RowBox[{"data", ",", "dataCircle"}], "]"}]}], ";", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "[", "complement", "]"}], "\[Equal]", "0"}], ",", 
      RowBox[{"complement", "=", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"1", ",", "1"}], "}"}], "}"}]}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"p1", "=", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"Red", ",", 
         RowBox[{"PointSize", "[", 
          RowBox[{"0.007", "*", "c"}], "]"}], ",", 
         RowBox[{"Point", "[", "dataCircle", "]"}]}], "}"}], ",", 
       RowBox[{"Background", "\[Rule]", 
        RowBox[{"RGBColor", "[", 
         RowBox[{
          RowBox[{"0.5", "+", 
           RowBox[{"Mod", "[", 
            RowBox[{"b", ",", "2"}], "]"}]}], ",", 
          RowBox[{"Mod", "[", 
           RowBox[{"b", ",", "3"}], "]"}], ",", 
          RowBox[{"Mod", "[", 
           RowBox[{"b", ",", "5"}], "]"}]}], "]"}]}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"p2", "=", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"Blue", ",", 
         RowBox[{"PointSize", "[", 
          RowBox[{"0.007", "*", "c"}], "]"}], ",", 
         RowBox[{"Point", "[", "complement", "]"}]}], "}"}], ",", 
       RowBox[{"Background", "\[Rule]", 
        RowBox[{"RGBColor", "[", 
         RowBox[{
          RowBox[{"0.5", "+", 
           RowBox[{"Mod", "[", 
            RowBox[{"b", ",", "2"}], "]"}]}], ",", 
          RowBox[{"Mod", "[", 
           RowBox[{"b", ",", "3"}], "]"}], ",", 
          RowBox[{"Mod", "[", 
           RowBox[{"b", ",", "5"}], "]"}]}], "]"}]}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"app", "=", 
     RowBox[{"4", "*", 
      RowBox[{
       RowBox[{"Length", "[", "dataCircle", "]"}], "/", "a"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Column", "[", 
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Text", "@", 
         RowBox[{"Style", "[", 
          RowBox[{
           RowBox[{"Row", "[", 
            RowBox[{"{", 
             RowBox[{
             "\"\<\[Pi] \[TildeTilde] \>\"", ",", "app", ",", "\"\< = \>\"", 
              ",", 
              RowBox[{"N", "[", 
               RowBox[{"app", ",", "6"}], "]"}]}], "}"}], "]"}], ",", "Bold", 
           ",", "20"}], "]"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"Show", "[", 
         RowBox[{"p2", ",", "p1", ",", 
          RowBox[{"ImageSize", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{"350", ",", "350"}], "}"}]}]}], "]"}]}], "}"}], ",", 
      "\[IndentingNewLine]", "Center"}], "]"}]}], ",", "\[IndentingNewLine]", 
   
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", "10000", ",", "\"\<number of sample points\>\""}], 
      "}"}], ",", "40", ",", "20000", ",", "1", ",", 
     RowBox[{"Appearance", "\[Rule]", "\"\<Labeled\>\""}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"b", ",", "3", ",", "\"\<graph background color\>\""}], "}"}], 
     ",", "0", ",", "10", ",", "1", ",", 
     RowBox[{"Appearance", "\[Rule]", "\"\<Labeled\>\""}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"c", ",", "1", ",", "\"\<point size\>\""}], "}"}], ",", "0.8", 
     ",", "2.4", ",", "0.1", ",", 
     RowBox[{"Appearance", "\[Rule]", "\"\<Labeled\>\""}]}], "}"}], ",", 
   RowBox[{"TrackedSymbols", "\[Rule]", "Manipulate"}]}], "]"}]], "Input",
 CellChangeTimes->{
