$CellContext`sol], {$CellContext`i, 0, $CellContext`M}]; $CellContext`mu = 
            Sum[($CellContext`i/$CellContext`M) 
              Part[$CellContext`p, $CellContext`i + 1], {$CellContext`i, 
               0, $CellContext`M}]; $CellContext`variance = 
            Sum[($CellContext`i/$CellContext`M - $CellContext`mu)^2 
              Part[$CellContext`p, $CellContext`i + 1], {$CellContext`i, 
               0, $CellContext`M}]; $CellContext`S = -
             Sum[Part[$CellContext`p, $CellContext`i + 1] Log[
                 Part[$CellContext`p, $CellContext`i + 1]], {$CellContext`i, 
                0, $CellContext`M}]; 
           ListPlot[$CellContext`p, PlotRange -> {-0.005, 0.12}, 
             DataRange -> {0, 1}, Axes -> False, Frame -> True, PlotLabel -> 
             Row[{
                Style["S", Italic], " \[TildeTilde] ", $CellContext`S}], 
             FrameLabel -> {
               Row[{
                 Style["\!\(\*SubscriptBox[\(p\), \(d\)]\)", Italic], " = ", 
